/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 


.clearfix::after{
  content: '';
  display: block;
  clear: both;
  line-height: 0;
  height: 0;
  visibility:0; 
}
button,input{
  margin: 0;
  padding: 0;
  line-height: normal;
}
